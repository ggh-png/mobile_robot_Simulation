<launch>
   <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
	    <rosparam file="$(find marvel_v1)/param/costmap_common_params.yaml" command="load" ns="global_costmap" /> 
	    <rosparam file="$(find marvel_v1)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
	    <rosparam file="$(find marvel_v1)/param/local_costmap_params.yaml" command="load" />
	    <rosparam file="$(find marvel_v1)/param/global_costmap_params.yaml" command="load" /> 
	    <rosparam file="$(find marvel_v1)/param/base_local_planner_params.yaml" command="load" />
	</node>
</launch>
